{
    "Template d'Algorithme": {
        "prefix": "algo",
        "body": [
            "Variable",
            "${2:variable}: ${3:type}",
            "",
            "Debut",
            "\t${0}",
            "Fin"
        ],
        "description": "Template basique d'Algo"
    },
    "If Statement": {
        "prefix": "si",
        "body": [
            "Si ${1:condition} Alors",
            "\t${2}",
            "FinSi"
        ],
        "description": "Instruction Si"
    },
    "If-Else Statement": {
        "prefix": "sisi",
        "body": [
            "Si ${1:condition} Alors",
            "\t${2}",
            "Sinon",
            "\t${3}",
            "FinSi"
        ],
        "description": "Instruction Si-Sinon"
    },
    "If-ElseIf-Else Statement": {
        "prefix": "sisinon",
        "body": [
            "Si ${1:condition1} Alors",
            "\t${2}",
            "SinonSi ${3:condition2} Alors",
            "\t${4}",
            "Sinon",
            "\t${5}",
            "FinSi"
        ],
        "description": "Instruction Si-SinonSi-Sinon"
    },
    "For Loop": {
        "prefix": "pour",
        "body": [
            "Pour ${1:i} De ${2:debut} A ${3:fin} Pas ${4:1} Faire",
            "\t${5}",
            "FinPour"
        ],
        "description": "Boucle Pour"
    },
    "While Loop": {
        "prefix": "tantque",
        "body": [
            "TantQue ${1:condition} Faire",
            "\t${2}",
            "FinTantQue"
        ],
        "description": "Boucle TantQue"
    },
    "Switch Statement": {
        "prefix": "selon",
        "body": [
            "Selon ${1:expression} Faire",
            "\tCas ${2:valeur1}:",
            "\t\t${3}",
            "\tCas ${4:valeur2}:",
            "\t\t${5}",
            "\tSinon",
            "\t\t${6}",
            "FinSelon"
        ],
        "description": "Instruction Selon"
    },
    "Array Declaration 1D": {
        "prefix": "tab1",
        "body": [
            "Tableau ${1:nom}[${2:taille}]: ${3:type}"
        ],
        "description": "Tableau 1D"
    },
    "Array Declaration 2D": {
        "prefix": "tab2",
        "body": [
            "Tableau ${1:nom}[${2:lignes}][${3:colonnes}]: ${4:type}"
        ],
        "description": "Tableau 2D"
    },
    "Function": {
        "prefix": "fonction",
        "body": [
            "Fonction ${1:nom}(${2:parametres}): ${3:type_retour}",
            "Variable",
            "\t${4:variables_locales}",
            "Debut",
            "\t${5}",
            "\tRetourne ${6:valeur}",
            "FinFonction"
        ],
        "description": "Fonction avec retour"
    },
    "Procedure": {
        "prefix": "procedure",
        "body": [
            "Procedure ${1:nom}(${2:parametres})",
            "Variable",
            "\t${3:variables_locales}",
            "Debut",
            "\t${4}",
            "FinProcedure"
        ],
        "description": "Procedure sans retour"
    },
    "Module": {
        "prefix": "module",
        "body": [
            "Module ${1:nom}",
            "",
            "// Déclarations globales",
            "Variable",
            "\t${2:variables_globales}",
            "",
            "${3:// Fonctions et procédures du module}",
            "",
            "FinModule"
        ],
        "description": "Module"
    },
    "Import Module": {
        "prefix": "importer",
        "body": [
            "Importer ${1:nom_module}"
        ],
        "description": "Importer un module"
    }
}